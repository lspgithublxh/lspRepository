<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
  
  <sql-query name="getAllUser">
   <![CDATA[ 
          SELECT  
                 {u.*}  
           FROM  
                user u  
           WHERE  
                 u.name = :name  
      ]]>  
      <return alias="u" class="com.yunhai.model.UserEntity"></return>
  </sql-query>
 </hibernate-mapping>